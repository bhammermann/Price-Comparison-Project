<html>
    <body>
        <h1>GPUs</h1>
        <ul>
            <% GPU.forEach(function(gpu){ %>
                <li>
                    <h6><%= gpu.name %></h6>
                    <p><%= gpu.preis %></p>
                    <% if (gpu.image) { %>
                        <img src="<%= gpu.image %>" />
                      <% } %>
                </li>
            <% }); %>
        </ul>


        <h1>CPUs</h1>
